<div id="monthly_posts">
	<table>
    
    <% @monthly_posts.each do |post| %>
    <tr>
      <td>
        <%= post.created_at.strftime("%m-%d-%Y") %>
  			<%= markdown(post.body) %> <%= link_to "Edit Post", edit_admin_monthly_post_path(post) %> |
  			<%= link_to "Delete Post", admin_monthly_post_path(post), :method => :delete,
:confirm => "Are you sure you want to delete this Post?" %> <br />
			</td>
		</tr>
			<% end %>
		
	</table>


<%= will_paginate @monthly_posts %>
</div>