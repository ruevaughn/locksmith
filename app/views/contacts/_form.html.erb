<%= form_for(@contact) do |f| %>

<p>
  <%= f.label(:phone) %>
  <%= f.text_field :phone %>
</p>

<p>
  <%= f.label(:email) %>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label(:facebook) %>
  <%= f.text_field :facebook %>
</p>

<p>
  <%= f.label(:twitter) %>
  <%= f.text_field :twitter %>
</p>
<p>
  <%= f.label(:review) %>
  <%= f.text_field :review %>
</p>


<p>
  <%= f.submit("Submit") %>
</p>
<% end %>